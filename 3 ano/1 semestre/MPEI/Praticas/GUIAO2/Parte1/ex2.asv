% exercicio 2 -> parte 2.1



% alinea a)
m = 100 ; 
n = 20 ;
N = 10000;

experiencia = randi(m,n,N);

diferent = zeros(1,N);

for col = 1:N
    diferent(col)=length(unique(experiencia(:,col))) == n;
end

p1 = sum(diferent)/N;


fprintf("Resultado alinea a): %1.4f\n",p1);


% alinea b)
p2 = 1 - p1;

fprintf("Resultado alinea b): %1.4f\n",p2);



